    <initial target="../1">
     <action>Q_UNUSED_PAR(par);

#ifdef Q_SPY
Philo *Philo_inst = me;
QS_OBJ_ARR_DICTIONARY(Philo_inst, me-&gt;id);
QS_OBJ_ARR_DICTIONARY(&amp;Philo_inst-&gt;timeEvt, me-&gt;id);
