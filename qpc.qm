//! @private @memberof QEvt
static inline void QEvt_refCtr_inc_(QEvt const *me) {
    uint8_t rc = me-&gt;refCtr_ + 1U;
    ((QEvt *)me)-&gt;refCtr_ = rc; // cast away 'const'
#ifndef Q_UNSAFE
    ((QEvt *)me)-&gt;evtTag_ = (me-&gt;evtTag_ &amp; 0xF0U) | ((~rc) &amp; 0x0FU);
