Q_UNUSED_PAR(evtRef);
#endif

QF_CRIT_STAT
QF_CRIT_ENTRY();

Q_REQUIRE_INCRIT(500, e != (QEvt *)0);
#ifndef Q_UNSAFE
Q_INVARIANT_INCRIT(501, QEvt_verify_(e));
#endif

uint_fast8_t const poolNum = QEvt_getPoolNum_(e);
Q_UNUSED_PAR(poolNum); // might be unused

Q_REQUIRE_INCRIT(510, (poolNum != 0U)
    &amp;&amp; (evtRef == (void *)0));

