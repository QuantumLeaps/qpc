Q_INVARIANT_INCRIT(702, tickRate == dis);
#endif // ndef Q_UNSAFE

QACTIVE_CAST_(me)-&gt;eQueue.tail = 0U; // clear # ticks
#ifndef Q_UNSAFE
QACTIVE_CAST_(me)-&gt;eQueue.tail_dis = (QEQueueCtr)~0U;
#endif // ndef Q_UNSAFE

