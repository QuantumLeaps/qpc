QS_MEM_SYS();
QS_BEGIN_PRE(QS_QF_NEW_REF,
        (uint_fast8_t)QS_EP_ID + poolNum)
    QS_TIME_PRE();       // timestamp
    QS_SIG_PRE(e-&gt;sig);  // the signal of the event
    QS_2U8_PRE(poolNum, e-&gt;refCtr_);
QS_END_PRE()
QS_MEM_APP();

QF_CRIT_EXIT();

return e;</code>
   </operation>
